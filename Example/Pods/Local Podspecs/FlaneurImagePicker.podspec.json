{
  "name": "FlaneurImagePicker",
  "version": "1.0.0",
  "summary": "FlaneurImagePicker is an iOS image picker",
  "description": "FlaneurImagePicker is an iOS image picker that allows users to pick images from different sources (ex: user's library, user's camera, instagram ...).\nIt's highly customizable.",
  "homepage": "https://github.com/toto/FlaneurImagePicker",
  "screenshots": "https://raw.githubusercontent.com/toto/FlaneurImagePicker/master/images/logo.png",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Matthieu Luci": "matthieu.luci@gmail.com"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source": {
    "git": "https://github.com/toto/FlaneurImagePicker.git",
    "tag": "1.0.0"
  },
  "module_name": "FlaneurImagePicker",
  "source_files": [
    "Sources/**/*.swift",
    "Sources/**/**/*.swift"
  ],
  "resource_bundles": {
    "FlaneurImagePicker": "Sources/Assets/*"
  },
  "requires_arc": true,
  "frameworks": [
    "UIKit",
    "Photos"
  ],
  "dependencies": {
    "IGListKit": [
      "~> 3.0"
    ],
    "ActionKit": [
      "~> 2.0"
    ],
    "Kingfisher": [
      "~> 3.10"
    ]
  },
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "3.0"
  }
}
